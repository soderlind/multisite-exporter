{
	"name": "soderlind/multisite-exporter",
	"description": "Runs WordPress Exporter on each subsite in a multisite, in the background.",
	"type": "wordpress-plugin",
	"keywords": [
		"wordpress",
		"plugin",
		"multisite",
		"exporter",
		"wp-cli",
		"wp-cli-package"
	],
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Per SÃ¸derlind",
			"email": "email@example.com",
			"homepage": "https://soderlind.no"
		}
	],
	"require": {
		"php": ">=7.2",
		"woocommerce/action-scheduler": "^3.5",
		"yahnis-elsts/plugin-update-checker": "^5.5"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true
		}
	},
	"autoload": {
		"files": [
			"multisite-exporter.php"
		]
	},
	"extra": {
		"commands": [
			"multisite-exporter"
		],
		"bundled": false,
		"wp-cli": {
			"require-wp": true,
			"require-wp-multisite": true,
			"command": "multisite-exporter"
		}
	}
}